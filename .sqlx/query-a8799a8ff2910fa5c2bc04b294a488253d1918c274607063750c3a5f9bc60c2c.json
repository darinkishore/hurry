  "query": "\n            SELECT i.id, i.organization_id, r.name as role_name, i.expires_at,\n                   i.max_uses, i.use_count, i.revoked_at\n            FROM organization_invitation i\n            JOIN organization_role r ON i.role_id = r.id\n            WHERE i.token = $1\n            FOR UPDATE\n            ",
